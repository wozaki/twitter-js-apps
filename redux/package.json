{
  "name": "twitter-redux",
  "private": true,
  "description": "Twitter Client Using Redux",
  "version": "0.0.1",
  "main": "app/main/browser/index.js",
  "scripts": {
    "prestart": "npm run compile",
    "start": "electron --debug=5858 .",
    "compile": "gulp compile",
    "test": "NODE_ENV=test mocha src/test/ --compilers js:babel-core/register --recursive",
    "clean": "rm -rf packages/v0.0.1/twitter-redux-{darwin,linux,win32}-x64",
    "package": "electron-packager . twitter-redux --arch=x64 --out=packages/ --platform=darwin,linux,win32 --version=0.30.6 --ignore=packages/*",
    "release": "npm run package && npm run zip && npm run clean && npm run upload",
    "setup": "npm install && brew tap tcnksm/ghr && brew install ghr && npm run compile",
    "upload": "ghr v0.0.1 packages/",
    "watch": "gulp watch",
    "zip": "npm run zip-darwin && npm run zip-win32 && npm run zip-linux",
    "zip-darwin": "cd packages && zip -r twitter-redux-darwin-x64.zip twitter-redux-darwin-x64",
    "zip-win32": "cd packages && zip -r twitter-redux-win32-x64.zip twitter-redux-win32-x64",
    "zip-linux": "cd packages && zip -r twitter-redux-linux-x64.zip twitter-redux-linux-x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wozaki/twitter-js-app.git"
  },
  "author": "Shun Ozaki <ozaki.shun5@gmail.com> (https://github.com/wozaki)",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.2",
    "eventemitter2": "^0.4.14",
    "font-awesome": "^4.4.0",
    "history": "^1.17.0",
    "html5-reset": "^2.1.3",
    "key-string": "^0.1.0",
    "lodash": "^4.15.0",
    "material-ui": "^0.13.1",
    "moment": "^2.10.6",
    "node-twitter-api": "^1.6.0",
    "qiita-coat": "^0.1.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redux": "^3.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^1.0.0",
    "spinkit": "^1.2.5",
    "twitter": "^1.2.5",
    "twitter-text": "^1.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-plugin-rewire": "^1.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "devtron": "^1.3.0",
    "electron-prebuilt": "^1.3.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^1.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sym": "0.0.14",
    "gulp-watch": "^4.3.5",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "rewire": "^2.5.2",
    "sinon": "^1.17.2"
  }
}
