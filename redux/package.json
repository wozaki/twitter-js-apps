{
  "name": "twitter-redux",
  "private": true,
  "description": "Twitter Client Using Redux",
  "version": "0.0.1",
  "main": "app/main/browser/index.js",
  "scripts": {
    "start": "electron .",
    "compile": "gulp compile",
    "clean": "rm -rf packages/v0.0.1/twitter-redux-{darwin,linux,win32}-x64",
    "package": "electron-packager . twitter-redux --arch=x64 --out=packages/ --platform=darwin,linux,win32 --version=0.30.6 --ignore=packages/*",
    "release": "npm run package && npm run zip && npm run clean && npm run upload",
    "setup": "npm install && brew tap tcnksm/ghr && brew install ghr && npm run compile",
    "upload": "ghr v0.0.1 packages/",
    "watch": "gulp watch",
    "zip": "npm run zip-darwin && npm run zip-win32 && npm run zip-linux",
    "zip-darwin": "cd packages && zip -r twitter-redux-darwin-x64.zip twitter-redux-darwin-x64",
    "zip-win32": "cd packages && zip -r twitter-redux-win32-x64.zip twitter-redux-win32-x64",
    "zip-linux": "cd packages && zip -r twitter-redux-linux-x64.zip twitter-redux-linux-x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wozaki/twitter-js-app.git"
  },
  "author": "Shun Ozaki <ozaki.shun5@gmail.com> (https://github.com/wozaki)",
  "license": "MIT",
  "dependencies": {
    "HTML5-Reset": "git://github.com/r7kamura/HTML5-Reset.git",
    "classnames": "^2.1.2",
    "eventemitter2": "^0.4.14",
    "font-awesome": "^4.4.0",
    "lodash": "^3.10.1",
    "material-ui": "^0.13.1",
    "moment": "^2.10.6",
    "node-twitter-api": "^1.6.0",
    "qiita-coat": "git://github.com/increments/qiita-coat.git#d751df7df60d0dcd0c97b9dc05c8ab5da3db22e3",
    "react": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "twitter": "^1.2.5",
    "twitter-text": "^1.13.0"
  },
  "devDependencies": {
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4",
    "babel-plugin-react-transform": "^1.1.0",
    "electron-prebuilt": "^0.36.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sym": "0.0.14",
    "gulp-watch": "^4.3.5"
  }
}
